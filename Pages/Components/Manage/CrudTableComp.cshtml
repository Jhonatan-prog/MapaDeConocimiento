<div class="table-container">
    <div class="queries-container">
        <div class="query-input-container">
            <input type="text", placeholder="Insert text" id="search-r-input">
            <button class="search-btn" id="search-btn">Search</button>
        </div>
        <div class="query-button-container">
            <button class="list-btn" id="list-btn">List</button>
            <button class="create-btn" id="create-btn">Create</button>
        </div>
    </div>
    <table class="register-table" id="registers-table" table-name="proyecto">
        <thead>
            @*    loaded header      *@
        </thead>
        <tbody>
            @*   Loaded registers    *@
        </tbody>
    </table>
    <div class="pag">
        <button class="prev">
            @* <img src="~/images/left-arrow.png" alt="left-arrow" class="left-arrow" asp-append-version="true"> *@
            Prev   
        </button>
        <div class="pages-container">
            @* buttons *@
        </div>
        <button class="next">
            @* <img src="~/images/right-arrow.png" alt="right-arrow" class="right-arrow" asp-append-version="true"> *@
            Next    
        </button>
    </div>
</div>

<div class="table-pages">
