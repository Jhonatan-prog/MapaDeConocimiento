    <button class="table-c" id="aliado_proyecto" table-name="aliado_proyecto">Aliado-proyecto</button>
    <button class="table-c" id="aa_proyecto" table-name="aa_proyecto">AA-proyecto</button>
    <button class="table-c" id="proyecto_linea" table-name="proyecto_linea">Proyecto-linea</button>
    <button class="table-c" id="ods_proyecto" table-name="ods_proyecto">Ods-proyecto</button>
    <button class="table-c" id="palabras_clave" table-name="palabras_clave">Palabras clave</button>
    <button class="table-c" id="producto" table-name="producto">Producto</button>

    @* No FK *@
    <button class="table-c" id="proyecto" table-name="proyecto">Proyecto</button>
    <button class="table-c" id="tipo_proyecto" table-name="tipo_producto">Tipo Producto</button>
